{
  "name": "@grexie/pages-plugin-markdown",
  "version": "1.0.50",
  "license": "MIT",
  "type": "module",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "babel src/ --no-copy-ignored -D -d ./lib/ -x .ts,.tsx --ignore '**/*.d.ts','**/__snapshots__/*'",
    "start": "npm run -s build -- -w"
  },
  "peerDependencies": {
    "react": ">=18.2.0"
  },
  "dependencies": {
    "@grexie/pages": "^1.0.50",
    "@mdx-js/loader": "^2.3.0",
    "@next/mdx": "^13.4.4",
    "mdast-excerpt": "^0.0.10",
    "rehype-stringify": "^10.0.0",
    "remark": "^15.0.1",
    "remark-frontmatter": "^4.0.1",
    "remark-html": "^16.0.1",
    "remark-mdx-next": "^0.1.1",
    "remark-mdx-to-plain-text": "^3.0.0"
  },
  "exports": {
    ".": {
      "default": "./lib/index.js"
    },
    "./remark": {
      "default": "./lib/remark.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "devDependencies": {
    "@types/js-yaml": "^4.0.5"
  }
}

{
  "name": "@grexie/pages",
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "babel src/ --no-copy-ignored -D -d ./lib/ -x .ts,.tsx --ignore '**/*.d.ts','**/__snapshots__/*'",
    "start": "npm run -s build -- -w"
  },
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "types": "./lib/index.d.ts"
    },
    "./next": {
      "import": "./lib/next/index.js",
      "types": "./lib/next/index.d.ts"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "dependencies": {
    "@grexie/context": "^0.4.1",
    "@grexie/hash-object": "^0.4.1",
    "next": "^13.4.2"
  }
}

{
  "name": "@grexie/pages",
  "version": "0.0.14",
  "license": "MIT",
  "type": "module",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "run-p -s build:ts build:ts-test",
    "build:ts": "babel src/ --no-copy-ignored -D -d ./lib/ -x .ts,.tsx --ignore '**/*.d.ts','**/*.test.ts','**/*.spec.ts','**/*.test.tsx','**/*.spec.tsx'",
    "build:ts-test": "babel src/ --no-copy-ignored -D -d ./lib/ -x .ts,.tsx --out-file-extension .cjs --only '**/*.test.tsx','**/*.test.ts','**/*.spec.tsx','**/*.spec.ts'",
    "watch": "run-p -s watch:ts watch:ts-test",
    "watch:ts": "npm run -s build:ts -- -w",
    "watch:ts-test": "npm run -s build:ts-test -- -w"
  },
  "peerDependencies": {
    "react": ">=18.2.0",
    "react-dom": ">=18.2.0"
  },
  "dependencies": {
    "@grexie/compose": "^0.1.16",
    "@grexie/context": "^0.0.13",
    "@grexie/hash-object": "^0.0.13",
    "@grexie/proxy": "^0.0.13",
    "react-is": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "@types/react-is": "^17.0.3"
  },
  "exports": {
    ".": {
      "default": "./lib/index.js",
      "types": "./lib/index.d.ts"
    },
    "./api": {
      "default": "./lib/api/index.js",
      "types": "./lib/api/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false
}

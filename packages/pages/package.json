{
  "name": "@grexie/pages",
  "version": "0.0.35",
  "license": "MIT",
  "type": "module",
  "module": "lib/index.js",
  "types": "lib/external.d.ts",
  "scripts": {
    "build": "babel src/ -D -d ./lib/ -x .ts,.tsx --ignore '**/*.d.ts','**/__snapshots__/*'",
    "watch": "npm run -s build -- -w"
  },
  "dependencies": {
    "@grexie/compose": "^0.1.17",
    "@grexie/context": "^*",
    "@grexie/hash-object": "^*",
    "@grexie/pages-builder": "^*",
    "@grexie/pages-cli": "^*",
    "@grexie/proxy": "^*",
    "react": ">=18.2.0",
    "react-is": ">=18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@types/react-is": "^17.0.3"
  },
  "exports": {
    "./api": {
      "import": "./lib/api/index.js",
      "types": "./lib/api/index.d.ts",
      "default": "./lib/api/index.js"
    },
    "./defaults.pages": {
      "import": "./lib/defaults.pages.js",
      "types": "./lib/defaults.pages.d.ts",
      "default": "./lib/defaults.pages.js"
    },
    "./package.json": "./package.json",
    ".": {
      "@grexie/pages": "./lib/plugin.js",
      "import": "./lib/index.js",
      "types": "./lib/external.d.ts",
      "default": "./lib/index.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false
}

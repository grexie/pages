{
  "name": "@grexie/pages-cli",
  "version": "0.0.14",
  "license": "MIT",
  "bin": {
    "pages": "bin/index.js"
  },
  "type": "module",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "npm run -s generate:packages && babel src/ --no-copy-ignored -D -d ./lib/ -x .ts,.tsx --ignore '**/*.d.ts'",
    "watch": "npm run -s build -- -w",
    "generate:packages": "mkdir -p lib/utils && (cd ../.. && monorepo workspaces -pj) >lib/utils/packages.json"
  },
  "dependencies": {
    "chalk": "^5.1.2",
    "enhanced-resolve": "^5.10.0",
    "source-map-support": "^0.5.21"
  },
  "exports": {
    "./scripts/dev/link": "./lib/scripts/dev/link.js",
    "./scripts/dev/unlink": "./lib/scripts/dev/unlink.js",
    "./scripts/create": "./lib/scripts/create"
  },
  "publishConfig": {
    "access": "public"
  }
}

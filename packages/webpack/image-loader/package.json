{
  "name": "@grexie/pages-image-loader",
  "version": "0.0.13",
  "license": "MIT",
  "type": "module",
  "module": "lib/index.mjs",
  "main": "lib/index.cjs",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "run-p -s build:ts-mjs build:ts-cjs",
    "build:ts-mjs": "NODE_ENV=esm babel src/ --no-copy-ignored -D -d ./lib/ -x .ts,.tsx --out-file-extension .mjs --ignore '**/*.d.ts'",
    "build:ts-cjs": "NODE_ENV=commonjs babel src/ --no-copy-ignored -D -d ./lib/ --out-file-extension .cjs -x .ts,.tsx --ignore '**/*.d.ts'",
    "watch": "run-p -s watch:ts-mjs watch:ts-cjs",
    "watch:ts-mjs": "npm run -s build:ts-mjs -- -w",
    "watch:ts-cjs": "npm run -s build:ts-cjs -- -w"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "dependencies": {
    "@babel/core": "^7.20.2",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@svgr/core": "^6.5.1",
    "sharp": "^0.31.2"
  },
  "peerDependencies": {
    "@grexie/pages-builder": "^0.0.13",
    "webpack": "^5.75.0"
  },
  "exports": {
    ".": {
      "import": "./lib/index.mjs",
      "require": "./lib/index.cjs",
      "types": "./lib/index.d.ts",
      "default": "./lib/index.cjs"
    }
  }
}
